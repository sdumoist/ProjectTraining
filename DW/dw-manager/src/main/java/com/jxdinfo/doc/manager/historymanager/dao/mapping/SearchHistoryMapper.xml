<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jxdinfo.doc.manager.historymanager.dao.SearchHistoryMapper" >
    <!-- 往搜索记录表中插入数据 -->
    <insert id="insertIntoSearchHistory">
        INSERT INTO DOC_HISTORY_SEARCH (HISTORY_ID,USER_ID,KEYWORDS,CREATE_TIME,VALID_FLAG) VALUES (#{historyId},#{userId},#{keywords},#{searchTime},#{validFlag})
    </insert>
</mapper>